				<div class="col-md-9">
					<div class="row">

						{{#each productData}}
						<div class="col-md-4">
							<div class="product-item">
								<div class="product-thumb">
									{{!-- <span class="bage">HOT</span> --}}
									<img class="img-responsive" src="/uploads/productsImage/{{this.image}}"
										alt="product-img" />
									<div class="preview-meta">
										<ul>
											<li>
												<span data-toggle="modal" data-target="#p{{this._id}}">
													<i class="tf-ion-ios-search-strong"></i>
												</span>
											</li>
											<li>
												<a href="#!"><i class="tf-ion-ios-heart"></i></a>
											</li>
											<li>
												<a href="#!"><i class="tf-ion-android-cart"></i></a>
											</li>
										</ul>
									</div>
								</div>
								<div class="product-content">
									<h4><a href="product-single">{{this.name}}</a></h4>
									<p class="price">${{this.price}}</p>
								</div>
							</div>
						</div>
						<!-- Modal -->
						<div class="modal product-modal fade" id="p{{this._id}}">
							<button type="button" class="close" data-dismiss="modal" aria-label="Close">
								<i class="tf-ion-close"></i>
							</button>
							<div class="modal-dialog " role="document">
								<div class="modal-content">
									<div class="modal-body">
										<div class="row">
											<div class="col-md-8 col-sm-6 col-xs-12">
												<div class="modal-image">
													<img class="img-responsive"
														src="/uploads/productsImage/{{this.image}}" alt="product-img" />
												</div>
											</div>
											<div class="col-md-4 col-sm-6 col-xs-12">
												<div class="product-short-details">
													<h2 class="product-title">{{this.name}}</h2>
													<p class="product-price">{{this.price}}</p>
													<p class="product-short-description">
														{{this.description}}
													</p>
													<a href="" class="btn btn-main">Add To Cart</a>
													<a href="product/detail/{{this._id}}"
														class="btn btn-transparent">View Product
														Details</a>
												</div>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
						<!-- /.modal -->
						{{/each}}
